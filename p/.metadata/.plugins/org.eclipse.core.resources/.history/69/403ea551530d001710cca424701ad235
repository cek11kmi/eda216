
public class GUI {
	private Database db;

	public GUI(Database db) {
		this.db = db;
		startDB();
	}
	
	private void startDB(){
		if (db.openConnection("cookies.db")) {
			System.out.println("Database connected");
			db.foreignKey();
		} else {
			System.out.println("Database not connected");
			System.exit(0);
}
	}
}
