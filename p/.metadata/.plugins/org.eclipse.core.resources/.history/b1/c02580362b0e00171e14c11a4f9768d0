import javafx.beans.property.SimpleStringProperty;

public class SearchPalletByProductDataHolder {

		private final SimpleStringProperty palletId;

		
		public SearchPalletByProductDataHolder(String palletId) {
			this.palletId = new SimpleStringProperty(palletId);
		}
		
		// GET
		public String getBikeBarcode() { return palletId.get();}

		
		// SET
		public void setBikeBarcode(String value) { palletId.set(value);}

		
	}

